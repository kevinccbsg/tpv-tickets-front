{
  "name": "tpv-tickets-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-storybook": "build-storybook -c .storybook -o ./static-storybook",
    "test:unit": "vue-cli-service test:unit",
    "test:cypress": "cd UI-e2e && npm run cy:open",
    "test:e2e": "cd UI-e2e && npm run cy:run",
    "lint": "vue-cli-service lint",
    "start": "node server.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@saeris/vue-spinners": "^1.0.8",
    "@storybook/vue": "^5.3.9",
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "core-js": "^3.4.4",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.19",
    "register-service-worker": "^1.7.1",
    "snyk": "^1.319.2",
    "vue": "^2.6.10",
    "vue-analytics": "^5.22.1",
    "vue-i18n": "^8.17.5",
    "vue-notification": "^1.3.20",
    "vue-router": "^3.1.6",
    "vuelidate": "^0.7.5",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-backgrounds": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addon-options": "^5.2.5",
    "@storybook/addon-storysource": "^5.2.5",
    "@storybook/addon-viewport": "^5.2.5",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "cypress-file-upload": "^3.5.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^4.2.1",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test:unit",
      "pre-push": "npm run test:e2e"
    }
  },
  "snyk": true
}
